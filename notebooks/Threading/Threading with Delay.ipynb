   ],
   "source": [
    "import threading\n",
    "import time\n",
    "\n",
    "def delayed_print(msg):\n",
    "    time.sleep(2)\n",
    "    print(msg)\n",
    "\n",
    "t = threading.Thread(target=delayed_print, args=(\"Hello, after delay!\",))\n",
    "t.start()"
   ]
  },
